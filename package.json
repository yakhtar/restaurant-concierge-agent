{
  "name": "restaurant-concierge-agent",
  "version": "1.0.0",
  "description": "AI-powered restaurant concierge agent with WhatsApp integration via n8n",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:simple": "tsx watch src/simple-index.ts",
    "dev:enhanced": "tsx watch src/enhanced-index.ts",
    "start": "node dist/index.js",
    "start:simple": "node dist/simple-index.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "mcp:google-places": "node dist/mcp-servers/google-places.js",
    "mcp:opentable": "node dist/mcp-servers/opentable.js",
    "mcp:customer-prefs": "node dist/mcp-servers/customer-preferences.js",
    "generate:restaurant": "tsx src/cli/generate-restaurant.ts",
    "restaurants:list": "tsx src/cli/generate-restaurant.ts list",
    "restaurants:preview": "tsx src/cli/generate-restaurant.ts preview"
  },
  "keywords": [
    "restaurant",
    "concierge",
    "whatsapp",
    "n8n",
    "mcp",
    "ai",
    "reservations",
    "google-places",
    "opentable"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@types/node": "^20.0.0",
    "axios": "^1.6.0",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "inquirer": "^12.9.4",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.0",
    "moment-timezone": "^0.5.0",
    "node-cron": "^3.0.0",
    "sqlite3": "^5.1.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.0",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.0",
    "@types/node-cron": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
